{
    "$id": "https://developer.microsoft.com/json-schemas/fabric/item/graphModel/definition/graphType/1.0.0/schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "GraphType",
    "type": "object",
    "properties": {
      "schemaVersion": {
        "type": "string",
        "pattern": "^\\d+\\.\\d+\\.\\d+$"
      },
      "nodeTypes": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "alias": {
              "type": "string"
            },
            "labels": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "primaryKeyProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "properties": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string",
                    "enum": ["STRING", "INT", "FLOAT", "ZONED DATETIME", "BOOLEAN"]
                  }
                },
                "required": ["name", "type"]
              }
            }
          },
          "required": ["alias", "labels", "primaryKeyProperties", "properties"]
        }
      },
      "edgeTypes": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "alias": {
              "type": "string"
            },
            "labels": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "sourceNodeType": {
              "type": "object",
              "properties": {
                "alias": {
                  "type": "string"
                }
              },
              "required": ["alias"]
            },
            "destinationNodeType": {
              "type": "object",
              "properties": {
                "alias": {
                  "type": "string"
                }
              },
              "required": ["alias"]
            },
            "properties": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string",
                    "enum": ["STRING", "INT", "FLOAT", "ZONED DATETIME", "BOOLEAN"]
                  }
                },
                "required": ["name", "type"]
              }
            }
          },
          "required": ["alias", "labels", "sourceNodeType", "destinationNodeType", "properties"]
        }
      }
    },
    "required": ["schemaVersion", "nodeTypes", "edgeTypes"]
  }